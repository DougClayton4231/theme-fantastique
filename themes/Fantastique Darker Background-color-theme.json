{
    "tokenColors": [
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "#B3B3B3"
            }
        },
        {
            "name": "Italic",
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Bold",
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Header and HTML tags",
            "scope": [
                "header",
                "entity.name.tag.html",
                "punctuation.definition.tag.begin.html"
            ],
            "settings": {
                "foreground": "#34BDE8"
            }
        },
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "foreground": "#737373"
            }
        },
        {
            "name": "Comment blocks",
            "scope": "comment.block",
            "settings": {
                "foreground": "#707A70"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.token"
            ],
            "settings": {
                "foreground": "#80B5E0"
            }
        },
        {
            "name": "Booleans",
            "scope": [
                "constant.language.boolean",
                "keyword.token.false",
                "keyword.token.true"
            ],
            "settings": {
                "foreground": "#4AE08C"
            }
        },
        {
            "name": "Null",
            "scope": [
                "constant.language.null",
                "keyword.token.null"
            ],
            "settings": {
                "foreground": "#ff7ae2"
            }
        },
        {
            "name": "Scope variables",
            "scope": [
                "constant.language.scope",
                "keyword.control.scope"
            ],
            "settings": {
                "foreground": "#F2CA27"
            }
        },
        {
            "name": "Constants (All Caps)",
            "scope": [
                "constant.other",
                "support.constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#F291C3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Traits and Attributes",
            "scope": [
                "support.constant.trait",
                "constant.language.trait",
                "support.constant.attribute"
            ],
            "settings": {
                "foreground": "#00D4D4"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "entity.name.operator.custom-literal.number",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#2eccb0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Hex and Binary Numbers",
            "scope": [
                "constant.numeric.hex.integer",
                "constant.numeric.integer.binary",
                "constant.numeric.integer.hexadecimal",
                "constant.numeric.hex.float",
                "constant.numeric.binary.integer",
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
                "constant.character.hex-string"
            ],
            "settings": {
                "foreground": "#FF936B"
            }
        },
        {
            "name": "Regexp",
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#b3b3b3"
            }
        },
        {
            "name": "Module and Import names",
            "scope": [
                "support.type.module",
                "entity.name.namespace",
                "variable.parameter.module",
                "variable.parameter.import"
            ],
            "settings": {
                "foreground": "#b3b3b3"
            }
        },
        {
            "name": "Function declarations",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.function.any-method",
                "support.constant.handlebars",
                "variable.function",
                "keyword.other.special-method",
                "meta.function-call.generic.python",
                "source.powershell variable.other.member"
            ],
            "settings": {
                "foreground": "#9acd32"
            }
        },
        {
            "name": "Language variables",
            "scope": [
                "storage.type",
                "entity.name.function.constructor",
                "entity.name.class.destructor",
                "entity.name.class.postblit",
                "support.type.vendored.property-name",
                "support.type.property-name",
